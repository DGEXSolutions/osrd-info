url: "/schemas/rolling_stock_schema.json",
async: false,
dataType: 'json',
success: function(data) {
rolling_stock = data;
}
});
// Render toggable list in the container element
document.getElementById("container2").appendChild(
renderjson(rolling_stock)
);
&lt;/script>
